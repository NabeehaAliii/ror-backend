    $('#tournament_id').click(function (){
        var radioValue = $("input[name='tournament_id']").val();
        $('#tournament_id').data('attribute',radioValue);
        $.ajax({
            type: "Get",
            url: "/tournament",
            data: { tournament_banner_id: radioValue},
            success: (response) => {
            }
        });
    });
</script>